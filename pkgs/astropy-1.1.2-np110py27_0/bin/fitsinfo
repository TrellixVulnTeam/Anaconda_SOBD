#!/opt/anaconda1anaconda2anaconda3/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'astropy==1.1.2','console_scripts','fitsinfo'
__requires__ = 'astropy==1.1.2'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('astropy==1.1.2', 'console_scripts', 'fitsinfo')()
    )
